  "devDependencies": {
    "@cloudflare/kv-asset-handler": "^0.2.0",
    "@types/fs-extra": "^9.0.13",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^18.7.13",
    "csso": "^5.0.5",
    "esbuild": "^0.15.5",
    "feed": "^4.2.2",
    "frontmatter": "0.0.3",
    "fs-extra": "^10.1.0",
    "globby": "^13.1.2",
    "got": "^12.3.1",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.4",
    "node-html-parser": "^5.4.1",
    "page-link-checker": "^1.3.0",
    "prettier": "^2.7.1",
    "remove-filename-hash": "^0.0.7",
    "schema-dts": "^1.1.0",
    "tempura": "^0.4.0",
    "tinydate": "^1.3.0",
    "tsm": "^2.2.2",
    "urlpattern-polyfill": "^5.0.6",
    "workbox-broadcast-update": "^6.5.4",
    "workbox-routing": "^6.5.4",
    "workbox-strategies": "^6.5.4",
    "workbox-streams": "^6.5.4",
    "wrangler": "^2.0.27"
  },
  "engines": {
    "node": ">=16"
  },
  "license": "Apache-2.0",
  "main": "dist/cf-sw.js",
  "name": "cf-worker",
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": false,
    "printWidth": 80,
    "quoteProps": "consistent",
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "useTabs": true
  },
  "private": true,
  "scripts": {
    "build": "tsm src/build/main.ts",
    "check-links": "tsm src/build/check-links.ts",
    "dev": "ENVIRONMENT_NAME=staging wrangler dev --env=staging",
    "format": "prettier --write \"{site,src,utils,static}/**/*.{ts,md,json,js}\"",
    "publish": "wrangler publish --env=$ENVIRONMENT_NAME",
    "stage": "ENVIRONMENT_NAME=staging npm run publish"
  },
  "version": "1.0.0",
  "volta": {
    "node": "16.17.0"
  }
