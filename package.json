{
  "private": true,
  "name": "cf-worker",
  "version": "1.0.0",
  "description": "An example of isomorphic service worker rendering.",
  "main": "dist/cf-sw.js",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "build": "esrun src/build/main.ts",
    "check-links": "esrun src/build/check-links.ts",
    "dev": "ENVIRONMENT_NAME=staging wrangler dev --env=staging",
    "format": "prettier --write \"{site,src,utils,static}/**/*.{ts,md,json,js}\"",
    "publish": "wrangler publish --env=$ENVIRONMENT_NAME",
    "stage": "ENVIRONMENT_NAME=staging npm run publish"
  },
  "author": "Jeff Posnick <blog@posnick.org>",
  "license": "Apache-2.0",
  "devDependencies": {
    "@cloudflare/kv-asset-handler": "^0.1.3",
    "@cloudflare/wrangler": "^1.18.0",
    "@digitak/esrun": "^1.2.6",
    "@types/fs-extra": "^9.0.12",
    "@types/markdown-it": "^12.0.3",
    "@types/node": "^16.4.0",
    "csso": "^4.2.0",
    "esbuild": "^0.12.15",
    "feed": "^4.2.2",
    "frontmatter": "0.0.3",
    "fs-extra": "^10.0.0",
    "globby": "^11.0.4",
    "markdown-it": "^12.1.0",
    "page-link-checker": "^1.3.0",
    "prettier": "^2.3.2",
    "schema-dts": "^0.9.0",
    "tempura": "^0.2.0",
    "tinydate": "^1.3.0",
    "urlpattern-polyfill": "^1.0.0-rc1",
    "workbox-broadcast-update": "^6.1.5",
    "workbox-routing": "^6.1.5",
    "workbox-strategies": "^6.1.5",
    "workbox-streams": "^6.1.5"
  }
}
